<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="ua.com.napps.scorekeeper"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity
            android:name="ua.napps.scorekeeper.View.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="ua.napps.scorekeeper.View.SettingsActivity"
            android:label="Settings"/>

        <activity android:name="ua.napps.scorekeeper.View.MainBetaActivity">
        </activity>
    </application>

</manifest>
